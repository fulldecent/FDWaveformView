FRAMEWORK_SEARCH_PATHS = $(inherited)  "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Expecta" "${PODS_ROOT}/Headers/Public/Expecta+Snapshots" "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase" "${PODS_ROOT}/Headers/Public/FDWaveformView" "${PODS_ROOT}/Headers/Public/Specta"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Expecta" -isystem "${PODS_ROOT}/Headers/Public/Expecta+Snapshots" -isystem "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase" -isystem "${PODS_ROOT}/Headers/Public/FDWaveformView" -isystem "${PODS_ROOT}/Headers/Public/Specta"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Expecta" -l"Expecta+Snapshots" -l"FBSnapshotTestCase" -l"FDWaveformView" -l"Specta" -framework "AVFoundation" -framework "CoreGraphics" -framework "Foundation" -framework "MediaPlayer" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/Pods